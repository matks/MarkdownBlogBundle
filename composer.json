{
    "name": "matks/markdown-blog-bundle",
    "type": "symfony-bundle",
    "description": "Markdown files based Bundle to integrate a simple blog in your Symfony2 application",
    "homepage": "https://github.com/matks/MarkdownBlogBundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [{"name": "Mathieu Ferment"}],
    "autoload": {
        "psr-4": {
            "Matks\\MarkdownBlogBundle\\": ""
        }
    },
    "prefer-stable": true,
    "require": {
        "php": ">=5.5",
        "symfony/symfony": "2.8.*",
        "erusev/parsedown": "^1.6"
    },
    "require-dev": {
        "atoum/atoum": "dev-master",
        "behat/symfony2-extension": "1.1.*"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
